Factorial
int factorial(int n)
{
	if (n < 0)
	{
		return (-1);
	}
	else if (n == 0)
	{
		return (1);
	}
	else
	{
		n = n * factorial(n - 1);
		return (n);
	}

	return (0);
}


Raiz cuadrada
int _sqrt_recursion(int n)
{
    int x;
    int a = (2 * x) - 1;
    
    if (n < 0)
    {
        return(-1);
    }
    else if (n == 0)
    {
        return(0);
    }
    
    if (x <= n)
    {
        
        a = a + _sqrt_recursion(x)
    }
}
